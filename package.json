{
  "name": "async-express-router",
  "version": "4.0.0",
  "description": "An express middleware for wrapping router methods (app.METHOD and express.Router()) with a customizable try / catch",
  "keywords": [
    "async",
    "express",
    "router",
    "api",
    "http",
    "error",
    "try catch",
    "wrapper"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "index.js",
  "repository": "https://github.com/matt-hahn/async-express-router.git",
  "author": "Matt Hahn <matt.hahn@matthahndesign.com>",
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "express": "4.x"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.4.0",
    "express": "^4.16.2",
    "mocha": "^4.1.0",
    "prettier": "^1.10.2",
    "supertest": "^3.0.0"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha test/app.js src/**/**.test.js",
    "test:watch": "NODE_ENV=test mocha --watch --recursive test/app.js src/**/**.test.js",
    "test:lint": "npm run eslint && npm test",
    "eslint": "eslint src/**"
  },
  "dependencies": {
    "express-api-methods": "^1.0.2",
    "express-router-methods": "^1.0.2",
    "is-async-await": "^1.0.2",
    "is-express-app": "^4.0.0",
    "isarray": "^2.0.2"
  }
}
